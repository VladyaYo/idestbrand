{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "hero": {
      "type": "component",
      "repeatable": false,
      "component": "sections.hero",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "projectName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "projectName",
      "required": true
    },
    "contentBlock": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.fullscreen-image",
        "sections.text",
        "sections.video",
        "sections.image",
        "sections.video-container"
      ]
    }
  }
}
